    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "amqp-ts": "^1.8.0",
