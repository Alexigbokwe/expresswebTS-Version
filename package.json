{
  "name": "typescript-project",
  "version": "1.0.0",
  "description": "",
  "main": "build/app.js",
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "ignore": [
      "./build/"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register app.ts",
    "ext": "ts, json"
  },
  "scripts": {
    "test": "jest",
    "dev": "npm run compile && nodemon",
    "start": "tsc && node -r tsconfig-paths/register -r ts-node/register build/app.js",
    "build": "rimraf ./build && npm run compile",
    "compile": "tsc -p .",
    "post:compile": "ef-tspm",
    "api-knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex --knexfile=./SchemaSetup.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/knex": "0.15.1",
    "@types/node": "^14.14.17",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "expresswebcorets": "^1.0.24",
    "expresswebjs-preset-ts": "^1.0.4",
    "maker-console-ts": "^1.0.10",
    "mongoose": "^5.11.9",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "socket.io": "^3.1.0"
  }
}
